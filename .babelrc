{
  "presets" : [
        "es2015"
      , "react"
    ]
  , "plugins" : [
    "react-html-attrs"
    , "transform-class-properties"
    , "transform-decorators-legacy"
    , "transform-object-rest-spread"
    , "babel-root-import"
  ]
  , "env": {
    "node" : {
      "plugins" : [
        "babel-plugin-transform-require-ignore"
      ]
    }
  }
}
